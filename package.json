{
  "name": "js-dev-env",
  "version": "1.0.0",
  "description": "JavaScript Development Environment from Pluralsight Course with Cory House",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch start-mockapi",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "share": "npm-run-all --parallel open:src localtunnel",
    "open:src": "babel-node buildScripts/srcServer.js",
    "localtunnel": "lt --port 3000",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "generate-mock-data": "babel-node buildScripts/generateMockData.js",
    "clean-dist": "rm -rf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tyqi11/js-dev-env.git"
  },
  "keywords": [],
  "author": "Tianyu Qi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tyqi11/js-dev-env/issues"
  },
  "homepage": "https://github.com/tyqi11/js-dev-env#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "babel-loader": "^8.3.0",
    "chai": "^4.2.0",
    "chalk": "^4.0.0",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "css-loader": "^6.7.3",
    "ejs": "^3.1.2",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-watch": "^6.0.1",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jquery": "^3.5.1",
    "jsdom": "^16.2.2",
    "json-schema-faker": "^0.5.0-rcv.24",
    "json-server": "^0.16.1",
    "mini-css-extract-plugin": "^2.7.2",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "open": "^7.0.3",
    "style-loader": "^1.2.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-md5-hash": "0.0.6",
    "whatwg-fetch": "^3.0.0"
  }
}
